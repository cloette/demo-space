{
  "name": "angular2-express-starter",
  "version": "1.0.0",
  "description": "Starter application of Angular4 with Express",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"npm run server:run\" \"ng serve --aot --progress=false --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test --watch false",
    "e2e:client": "ng e2e",
    "build": "ng build --prod --sm=false --aot --output-path=dist/client && npm run server:build",
    "server:run": "nodemon",
    "server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "author": "Vlado Tešanović",
  "engines": {
    "node": "8.9.1",
    "npm": "5.2.0"
  },
  "repository": {
    "url": "https://github.com/vladotesanovic/angular2-express-starter"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.3",
    "@angular/cdk": "^5.2.5",
    "@angular/cli": "^1.7.4",
    "@angular/common": "^6.1.3",
    "@angular/compiler": "^6.1.3",
    "@angular/compiler-cli": "^6.1.3",
    "@angular/core": "^6.1.3",
    "@angular/forms": "^6.1.3",
    "@angular/http": "^6.1.3",
    "@angular/language-service": "^6.1.3",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "^6.1.3",
    "@angular/platform-browser-dynamic": "^6.1.3",
    "@angular/router": "^6.1.3",
    "@ng-bootstrap/ng-bootstrap": "^3.0.0",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@ngx-gallery/core": "^3.1.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.33",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jasmine": "2.5.38",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/node": "^6.0.116",
    "@types/uuid": "^2.0.29",
    "auth0-js": "^9.7.3",
    "body-parser": "~1.17.1",
    "compression": "^1.7.3",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.5.7",
    "express": "^4.16.3",
    "express-jwt": "^5.1.0",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^5.2.9",
    "ngrx-store-freeze": "^0.2.4",
    "normalize.css": "^5.0.0",
    "path": "^0.12.7",
    "rxjs": "^6.2.2",
    "ts-helpers": "^1.1.2",
    "ts-node": "~3.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "codelyzer": "^4.0.2",
    "concurrently": "^3.6.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.5.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "^1.18.3",
    "protractor": "~5.1.0",
    "webdriver-manager": "10.2.5"
  }
}
